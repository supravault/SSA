{
  "name": "ssa-scanner",
  "version": "0.1.0",
  "description": "Supra-native Security-as-an-Agent scanner MVP",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ssa": "./dist/cli/ssa.js",
    "ssa-scan": "./dist/index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "clean": "rimraf dist",
    "dev": "tsx watch src/server.ts",
    "scan": "tsx src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:scan": "npm run build && node dist/scripts/test-scan.js",
    "test:view": "node scripts/test-view.js",
    "test:rules": "npm run build && node dist/scripts/test-rules-local.js",
    "test:fa": "npm run build && node dist/scripts/test-fa.js",
    "test:fa-probe": "npm run build && node dist/scripts/test-fa-view-probe.js",
    "test:fa:resources": "npm run build && node dist/src/scripts/test-fa-resources.js",
    "test:fa:surface": "npm run build && node dist/src/scripts/test-fa-surface.js",
    "test:coin": "npm run build && node dist/scripts/test-coin.js",
    "test:coin:surface": "npm run build && node dist/src/scripts/test-coin-surface.js",
    "test:rpc-modules": "npm run build && node dist/scripts/test-rpc-modules.js",
    "probe-rpc": "npm run build && node dist/src/scripts/probe-rpc-methods.js",
    "agent": "npm run build && node dist/src/scripts/ssa-agent.js",
    "monitor": "npm run build && node dist/src/index.js monitor --targets targets.json",
    "doctor": "node scripts/doctor.js",
    "guard:rpc": "node scripts/guard-rpc.js",
    "guard:chain": "node scripts/guard-chain-words.js",
    "guard:flavor": "node scripts/guard-flavor.js",
    "check:contamination": "node scripts/check-contamination.js",
    "prebuild": "npm run guard:rpc && npm run check:contamination",
    "scan:wallet": "npm run build && node dist/cli/ssa.js scan --kind wallet",
    "scan:coin": "npm run build && node dist/cli/ssa.js scan --kind coin",
    "scan:fa": "npm run build && node dist/cli/ssa.js scan --kind fa",
    "generate:badge-keys": "npm run build && node dist/scripts/generate-badge-keys.js",
    "verify-report": "npm run build && node dist/src/scripts/verify-report.js"
  },
  "keywords": [
    "supra",
    "move",
    "security",
    "scanner"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "11.1.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "pdfkit": "0.15.0",
    "tweetnacl": "1.0.3",
    "playwright": "1.40.0",
    "pdf-parse": "1.1.1",
    "qrcode": "1.5.3"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.10.6",
    "@types/pdfkit": "0.13.3",
    "@types/pdf-parse": "1.1.4",
    "@types/qrcode": "1.5.5",
    "@vitest/coverage-v8": "4.0.16",
    "rimraf": "6.0.0",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vitest": "4.0.16"
  }
}
